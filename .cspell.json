{
  "version": "0.2",
  "language": "en,es",
  "import": ["@cspell/dict-es-es/cspell-ext.json"],
  "dictionaries": ["en_us", "es-es", "project-words"],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./cspell-custom-dict.txt",
      "addWords": true
    }
  ],
  "words": [],
  "flagWords": [],
  "ignorePaths": [
    "**/*.lock",
    "**/*.json",
    "**/*.parquet",
    "**/*.xlsx",
    "**/*.ipynb",
    "**/htmlcov/**",
    "**/__pycache__/**",
    "**/*.egg-info/**",
    "**/.git/**",
    "**/uv.lock",
    "**/node_modules/**",
    "**/dist/**",
    "requirements.txt",
    "CLAUDE.md",
    "**/build/**"
  ],
  "overrides": [
    {
      "filename": "**/*.py",
      "language": "en,es"
    }
  ]
}
